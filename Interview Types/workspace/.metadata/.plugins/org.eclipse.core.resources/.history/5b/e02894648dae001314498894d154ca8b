package lab03;

public class Circle extends Shape{

	private int radius;
		
	public Circle(int radius){
		this.radius = radius;		
	}
	
	@Override
	public double getArea() {
		return Math.PI * Math.pow(radius, 2);
	}
	

	
	public String toString(){
		return this.getClass() + "-Radius: " + this.radius + "-Area: " 
				this.getArea();
	}

}
