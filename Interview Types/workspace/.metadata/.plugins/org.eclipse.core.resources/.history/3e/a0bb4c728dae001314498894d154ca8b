package lab03;

public class Square extends Rectangle{
	
	private int side;
	
	public Square(int side){
		super(side, side);
		this.side = side;
	}
	
	public String toString(){
		return this.getClass() + "-Square side: " + this.side + 
				this.getArea();
	}
	
//	@Override
//	public double getArea() {
//		return super.getArea();
//	}
	
}
