package dataStructures;

public class MyQueue<K>{
	
	private Node<K> head;
	private Node<K> tail;
	private int size;
	
	public MyQueue(){
		this.head = null;
		this.tail = null;
		this.size = 0;
	}
	
	public MyQueue(K k){
		Node<K> n = new Node<K>(k);
		this.head = n;
		this.tail = n;
		this.head.setNext(this.tail);
		this.size = 0;
	}
	
	public void enqueue(K k){
		if(this.head == null){
			this.head = new Node<K>(k);
		}			
		else{
			this.tail.setNext(new Node<K>(k));
			this.tail = this.tail.getNext();
		}
	}
	
//	public Node getHead() {
//		return head;
//	}
//	public void setHead(Node head) {
//		this.head = head;
//	}
//	public Node getTail() {
//		return tail;
//	}
//	public void setTail(Node tail) {
//		this.tail = tail;
//	}
//	public int getSize() {
//		return size;
//	}
//	public void setSize(int size) {
//		this.size = size;
//	}
	
	

}
